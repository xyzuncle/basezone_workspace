    <build>
        <plugins>
            <plugin>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-maven-plugin</artifactId>
            <configuration>
                <mainClass>com.kq.api.APIApplication</mainClass>
            </configuration>
            <!--  <executions>
                  <execution>
